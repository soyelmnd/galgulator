{
  "name": "galgulator",
  "version": "0.0.1",
  "description": "Not another Google calculator parody",
  "keywords": [
    "galgulator",
    "calculator",
    "google"
  ],
  "author": "soyelmnd <minhnd.it@gmail.com>",
  "license": "MIT",
  "config": {
    "less": {
      "paths": [
        "src/theme/less",
        "src/vendors"
      ]
    },
    "autoprefixer": {},
    "browserify": {
      "debug": true,
      "transform": [
        ["babelify", { "presets": ["es2015"] }]
      ]
    },
    "swig": {}
  },
  "path": {
    "source": {
      "base": "src/",
      "theme": "src/theme/less/",
      "script": "src/scripts/"
    },
    "build": {
      "base": "www/",
      "theme": "www/assets/css/",
      "script": "www/assets/js/"
    },
    "test": {
      "base": "tests/",
      "unit": "tests/units/"
    }
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "browserify": "^12.0.1",
    "connect": "^3.4.0",
    "connect-livereload": "^0.5.3",
    "gulp-inject": "^3.0.0",
    "gulp-swig": "^0.7.4",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "open": "0.0.5",
    "phantomjs": "^1.9.18",
    "request": "^2.65.0",
    "serve-static": "^1.10.0",
    "tiny-lr": "^0.2.1",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^3.30.0"
  }
}
